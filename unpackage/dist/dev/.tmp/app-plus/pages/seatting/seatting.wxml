<block wx:if="{{user.id}}"><view><block wx:if="{{flag}}"><view><cmd-cell-item vue-id="1" title="头像" arrow slot-right data-event-opts="{{[['^click',[['fnClickimg']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view><image class="userimg" src="{{user.faceImage}}" mode></image></view></cmd-cell-item><cmd-cell-item vue-id="2" title="昵称" arrow addon="{{user.nickname}}" data-event-opts="{{[['^click',[['fnClicknickname']]]]}}" bind:click="__e" bind:__l="__l"></cmd-cell-item><cmd-cell-item vue-id="3" title="生日" arrow slot-right data-event-opts="{{[['^click',[['fnClick']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view><picker mode="date" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{user.birthday}}</view></picker></view></cmd-cell-item><cmd-cell-item vue-id="4" title="性别" arrow addon="{{user.sex}}" data-event-opts="{{[['^click',[['fnClicksex']]]]}}" bind:click="__e" bind:__l="__l"></cmd-cell-item><button class="btn1" type="primary">清空缓存</button><button class="btn1" type="primary" data-event-opts="{{[['tap',[['toclear',['$event']]]]]}}" bindtap="__e">退出登录</button></view></block><block wx:else><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="biguserimg" bindtap="__e"><image src="{{user.faceImage}}" mode data-event-opts="{{[['longpress',[['long',['$event']]]]]}}" bindlongpress="__e"></image></view></block></view></block>